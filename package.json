{
  "name": "helloworld",
  "version": "1.0.2",
  "description": "Hello World Express Server",
  "main": "index.js",
  "scripts": {
    "compile": "npx tsc",
    "start": "node ./dist",
    "docker_build": "cd infra && node ../tools/commit-var GIT_COMMIT docker compose build",
    "docker_up": "docker compose --file infra/docker-compose.yml --project-directory . up",
    "docker_login": "aws ecr get-login-password --region us-west-2 | cross-var docker login --username AWS --password-stdin $npm_config_dockerregistry",
    "docker_push": "docker compose --file infra/docker-compose.yml --project-directory . push"
  },
  "author": "Ifeanyi Echeruo",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "cross-var": "^1.1.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
